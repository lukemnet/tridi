{
  "name": "tridi",
  "version": "1.2.1",
  "description": "JavaScript 360 3D Product Viewer",
  "main": "dist/js/tridi.min.js",
  "types": "dist/js/tridi.min.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "clean": "rimraf dist/js/*",
    "serve": "serve dist/",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -c tslint.json --project tsconfig.json src/*.ts",
    "build:es5": "tsc --outDir dist/js/es5",
    "build:es6": "tsc -m es6 --outDir dist/js/es6",
    "build:umd": "tsc",
    "build": "npm run clean && npm run build:es5 && npm run build:es6 && npm run build:umd && webpack",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/tridi.git"
  },
  "keywords": [
    "3d",
    "360",
    "image",
    "photo",
    "product",
    "view",
    "viewer"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/tridi/issues"
  },
  "homepage": "https://tridi.lukem.net",
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.16",
    "awesome-typescript-loader": "5.2.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "serve": "11.3.2",
    "terser-webpack-plugin": "5.0.3",
    "ts-jest": "26.4.4",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.1.3",
    "uglify-js": "3.12.3",
    "webpack": "5.11.1",
    "webpack-cli": "4.3.1"
  }
}
