{
  "name": "tridi",
  "version": "2.0.2",
  "description": "JavaScript 360 3D Product Viewer",
  "main": "dist/js/tridi.min.js",
  "types": "dist/js/tridi.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "clean": "rimraf dist/js/*",
    "serve": "serve dist/",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "build": "npm run clean && webpack",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/tridi.git"
  },
  "keywords": [
    "3d",
    "360",
    "image",
    "photo",
    "product",
    "view",
    "viewer"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/tridi/issues"
  },
  "homepage": "https://tridi.lukem.net",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "uglify-js": "^3.12.4",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1"
  }
}
